/**
 * 
 */
package trabajo_entornos;

public class Trabajo_Entornos {

	
	public static void main(String[] args) {
		
		int num1=3, num2=7;
		int recogeResultado = multiplicar(num1, num2);
		
		System.out.printf("Num1: %d * Num2: %d = Resultado: %d" ,num1, num2, recogeResultado);
		
		}
	
	public static int multiplicar (int n1, int n2) {
		
		int resultado;
		resultado = n1*n2;
		return resultado;
		
		
	}
 

}
